{
    "name": "heron/brasil",
    "description": "Brasil",
    "homepage": "https://siplaedu.com.br",
    "type": "project",
    "license": "Enterprise",
    "require": {
        "php": ">=7.4",
        "aelian/make-pdf": "dev-master",
        "cakephp/authentication": "^2.0",
        "cakephp/authorization": "^2.0",
        "cakephp/cakephp": "^4.5.0",
        "cakephp/migrations": "^3.7",
        "cakephp/plugin-installer": "^1.3",
        "mobiledetect/mobiledetectlib": "^3.74"
    },
    "require-dev": {
        "cakephp/bake": "^2.8",
        "cakephp/cakephp-codesniffer": "^4.5",
        "cakephp/debug_kit": "^4.9",
        "josegonzalez/dotenv": "^4.0",
        "phpunit/phpunit": "^9.6",
        "psy/psysh": "@stable",
        "phpunit/php-code-coverage": "^9.2",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "^2.8",
        "symfony/browser-kit": "^2.7",
        "symfony/css-selector": "^2.7",
        "phing/phing": "^2.12",
        "phpstan/phpstan": "@stable",
        "dereuromark/cakephp-ide-helper": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "MetronicV4\\": "plugins/MetronicV4/src/",
            "Pdf\\": "./plugins/Pdf/src/",
            "Pdf\\MakePdf\\": "vendor/aelian/make-pdf/src/",
            "CakeAes\\": "plugins/CakeAes/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "MetronicV4\\Test\\": "plugins/MetronicV4/tests/",
            "Pdf\\Test\\": "./plugins/Pdf/tests/",
            "CakeAes\\Test\\": "plugins/CakeAes/tests/"
        }
    },
    "scripts": {
        "install-codestandards": [
            "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
        ],
        "post-install-cmd": [
            "@install-codestandards"
        ],
        "cs-check": "vendor/bin/phpcs --colors -p -v --standard=build/phpcs.xml --extensions=php --ignore=autoload.php src/ templates/ tests/",
        "cs-fix": "vendor/bin/phpcbf --colors -p -v --standard=build/phpcs.xml --extensions=php --ignore=autoload.php src/ templates/ tests/",
        "stan": "vendor/bin/phpstan",
        "test": "vendor/bin/phpunit tests"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cakephp/plugin-installer": true
        }
    }
}